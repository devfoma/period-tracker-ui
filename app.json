{
  "expo": {
    "name": "Her Circle",
    "slug": "her-circle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "hercircle",
    "splash": {
      "backgroundColor": "#fdf8fa",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hercircle.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#fdf8fa"
      },
      "package": "com.hercircle.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"]
  }
}
